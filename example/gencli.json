{
    "fileExtension": "ts",
    "fileNameCase": "kebab",
    "folderNameCase": "kebab",
    "templatePath": "/src/meta/cli-templates",
    "entityConfigs": [
        {
            "name": "module",
            "prefix": "myapp",
            "path": "/src/store/modules",
            "fileConfigs": [
                {
                    "name": "*",
                    "template": "model.txt"
                },
                {
                    "name": "*-service",
                    "template": "service.txt"
                },
                {
                    "name": "*-module",
                    "template": "module.txt"
                },
                {
                    "name": "*-state",
                    "template": "state.txt"
                }
            ]
        },
        {
            "name": "component",
            "prefix": "myapp",
            "path": "/src/vues",
            "fileConfigs": [
                {
                    "name": "myapp-*",
                    "template": "vue.txt"
                },
                {
                    "name": "myapp-*",
                    "extension": "html"
                },
                {
                    "name": "myapp-*",
                    "extension": "scss",
                    "template": "style.txt"
                },
                {
                    "name": "myapp-*.spec",
                    "template": "vue-spec.txt"
                }
            ]
        }
    ]
}
