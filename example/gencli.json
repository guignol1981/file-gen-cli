{
    "fileNameCase": "kebab",
    "folderNameCase": "kebab",
    "accountKey": {
        // a valid service account key, this is to prevent publishing a write unrestricted key (ask me one!)
    },
    "entityConfigs": [
        {
            "name": "module",
            "prefix": "myapp",
            "fileConfigs": [
                {
                    "name": "*.ts",
                    "template": "model.txt"
                },
                {
                    "name": "*-service.ts",
                    "template": "service.txt"
                },
                {
                    "name": "*-module.ts",
                    "template": "module.txt"
                },
                {
                    "name": "*-state.ts",
                    "template": "state.txt"
                }
            ]
        },
        {
            "name": "component",
            "prefix": "myapp",
            "fileConfigs": [
                {
                    "name": "myapp-*.ts",
                    "template": "vue.txt"
                },
                {
                    "name": "myapp-*.html"
                },
                {
                    "name": "myapp-*.scss",
                    "template": "style.txt"
                },
                {
                    "name": "myapp-*.spec.ts",
                    "template": "vue-spec.txt"
                }
            ]
        }
    ]
}
