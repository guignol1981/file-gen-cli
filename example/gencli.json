{
    "cliName": "example-cli",
    "fileNameCase": "kebab",
    "folderNameCase": "kebab",
    "token": "ask me one!",
    "entityConfigs": [
        {
            "name": "module",
            "prefix": "myapp",
            "flat": true,
            "fileConfigs": [
                {
                    "name": "*.ts",
                    "template": "model.txt",
                    "path": "./model"
                },
                {
                    "name": "*-service.ts",
                    "template": "service.txt"
                },
                {
                    "name": "*-module.ts",
                    "template": "module.txt"
                },
                {
                    "name": "*-state.ts",
                    "template": "state.txt"
                }
            ]
        },
        {
            "name": "component",
            "prefix": "myapp",
            "fileConfigs": [
                {
                    "name": "myapp-*.ts",
                    "template": "vue.txt"
                },
                {
                    "name": "myapp-*.html"
                },
                {
                    "name": "myapp-*.scss",
                    "template": "style.txt"
                },
                {
                    "name": "myapp-*.spec.ts",
                    "template": "vue-spec.txt"
                }
            ]
        }
    ]
}
